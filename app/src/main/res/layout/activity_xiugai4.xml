<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffe9e9e9">
    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@+id/relativeLayout19">

        <ImageView
            android:id="@+id/imageView219"
            android:layout_width="match_parent"
            android:layout_height="230dp"
            android:contentDescription="@null"
            android:scaleType="fitXY"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:src="@drawable/beijing2"
            android:visibility="gone" />

        <ImageView
            android:id="@+id/imageView220"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:padding="1px"
            android:background="@android:color/white"
            android:contentDescription="@null"
            android:src="@drawable/avatar_default"
            android:layout_marginTop="37dp"
            android:layout_below="@+id/textView263"
            android:layout_centerHorizontal="true"
            android:visibility="gone" />

        <TextView
            android:id="@+id/textView227"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:singleLine="true"
            android:ellipsize="end"
            android:textSize="15dp"
            android:textColor="@android:color/white"
            android:layout_below="@+id/imageView220"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="10dp"
            android:visibility="gone" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView221"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:src="@drawable/ic_chevron_left_dark"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="5dp"
            android:visibility="gone" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="返回"
            android:id="@+id/textView228"
            android:layout_alignTop="@+id/imageView221"
            android:layout_toRightOf="@+id/imageView221"
            android:layout_toEndOf="@+id/imageView221"
            android:layout_marginTop="5dp"
            android:textColor="#ffffffff"
            android:visibility="gone" />

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="0.1dp"
            android:id="@+id/imageView222"
            android:layout_gravity="center_horizontal"
            android:background="#ffc2c2c2"
            android:layout_below="@+id/imageView219"
            android:visibility="gone" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="这家伙很懒，还没有编写简介"
            android:id="@+id/textView236"
            android:layout_below="@+id/textView227"
            android:layout_centerHorizontal="true"
            android:textColor="#ffffffff"
            android:layout_marginLeft="30dp"
            android:textSize="10dp"
            android:layout_marginTop="10dp"
            android:visibility="gone" />

        <ImageView
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:id="@+id/imageView226"
            android:layout_alignTop="@+id/textView236"
            android:layout_alignParentRight="false"
            android:layout_alignParentEnd="false"
            android:src="@drawable/ic_action_edit"
            android:layout_toRightOf="@+id/textView236"
            android:layout_marginLeft="10dp"
            android:visibility="gone" />

        <android.support.v4.view.ViewPager
            android:id="@+id/viewpager3"
            android:layout_width="match_parent"
            android:layout_height="fill_parent"
            android:layout_gravity="center"
            android:headerDividersEnabled="false"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/imageView219">

        </android.support.v4.view.ViewPager>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="退出"
            android:id="@+id/textView263"
            android:layout_below="@+id/imageView221"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="10dp"
            android:layout_marginRight="10dp"
            android:textColor="#ffffffff"
            android:visibility="gone" />
    </RelativeLayout>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:id="@+id/relativeLayout19"
        android:background="#fff4f4f4"
        android:layout_gravity="center_horizontal"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:visibility="gone">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="0.1dp"
            android:id="@+id/imageView71"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:background="#ff808080" />

        <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/imageView77"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="10dp"
            android:src="@drawable/ic_chevron_left" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="资料页"
            android:id="@+id/textView69"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="10dp"
            android:textColor="#ffdd7079" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="40dp"
        android:background="#85ffffff">

        <com.example.topnewgrid.obj.PagerSlidingTabStrip
            android:id="@+id/tabs"
            android:layout_width="wrap_content"
            app:pstsShouldExpand="false"
            app:pstsIndicatorHeight="2dp"
            app:pstsIndicatorColor="#ffff4c11"
            app:selectedTabTextColor="#ffff4c11"
            android:layout_height="40dp"
            android:layout_alignParentTop="true"

            android:layout_centerHorizontal="true" />
    </RelativeLayout>

</RelativeLayout>